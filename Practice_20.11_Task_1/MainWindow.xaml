<Window x:Class="Practice_20._11_Task_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practice_20._11_Task_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="False">
            <ToolBar DockPanel.Dock="Bottom">
                <Button Name="SaveButon" Content="Save" Click="SaveButon_Click"></Button>
                <Button Name="OpenButon" Content="Open" Click="OpenButon_Click"  ></Button>
                <Button Name="EditButon" Content="Edit" Click="EditButon_Click"></Button>
                <Button Name="PrintButon" Content="Print" Click="PrintButon_Click"></Button>
                <Button Name="ExitButon" Content="Exit" Click="ExitButon_Click"></Button>
            </ToolBar>
        </DockPanel>
        

    </Grid>
</Window>
